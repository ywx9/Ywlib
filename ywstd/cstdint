#pragma once
#include <stdint.h>

#ifndef ywstd_import
namespace std {
ywstd_export using int8_t = signed char;
ywstd_export using int16_t = short;
ywstd_export using int32_t = int;
ywstd_export using int64_t = long long;
ywstd_export using uint8_t = unsigned char;
ywstd_export using uint16_t = unsigned short;
ywstd_export using uint32_t = unsigned int;
ywstd_export using uint64_t = unsigned long long;
ywstd_export using int_least8_t = signed char;
ywstd_export using int_least16_t = short;
ywstd_export using int_least32_t = int;
ywstd_export using int_least64_t = long long;
ywstd_export using uint_least8_t = unsigned char;
ywstd_export using uint_least16_t = unsigned short;
ywstd_export using uint_least32_t = unsigned int;
ywstd_export using uint_least64_t = unsigned long long;
ywstd_export using int_fast8_t = signed char;
ywstd_export using int_fast16_t = int;
ywstd_export using int_fast32_t = int;
ywstd_export using int_fast64_t = long long;
ywstd_export using uint_fast8_t = unsigned char;
ywstd_export using uint_fast16_t = unsigned int;
ywstd_export using uint_fast32_t = unsigned int;
ywstd_export using uint_fast64_t = unsigned long long;
ywstd_export using intmax_t = long long;
ywstd_export using uintmax_t = unsigned long long;
}
#endif
